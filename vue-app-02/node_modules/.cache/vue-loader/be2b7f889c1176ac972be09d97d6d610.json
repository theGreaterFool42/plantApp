rd-title>{{plantName}}</v-card-title>\r\n\r\n    <v-card-text>\r\n      <v-row\r\n        align=\"center\"\r\n        class=\"mx-0\"\r\n      >\r\n        <v-rating\r\n          :value=\"4.5\"\r\n          color=\"amber\"\r\n          dense\r\n          half-increments\r\n          readonly\r\n          size=\"14\"\r\n        ></v-rating>\r\n      </v-row>\r\n\r\n      <div class=\"my-4 text-subtitle-1\">\r\n        <p>{{plantType}}</p>\r\n      </div>\r\n\r\n      <div>\r\n        <p>alive since {{plantDop}}</p>\r\n      </div>\r\n    </v-card-text>\r\n\r\n    <v-divider class=\"mx-4\"></v-divider>\r\n\r\n    <v-card-title>Look! Graphs!</v-card-title>\r\n    <v-slider\r\n        v-model=\"sliderValue\"\r\n        color=\"deep-purple lighten-1\"\r\n        thumb-label=\"always\">\r\n      </v-slider>\r\n    <v-card-text>\r\n      <v-chip-group\r\n        multiple\r\n        v-model=\"selection\"\r\n        active-class=\"deep-purple lighten-1 white--text\"\r\n        color=\"deep-purple lighten-1\"\r\n        column\r\n      >\r\n        <!-- <v-chip @click=\"chipHum = false,\r\n                        chipMois = false,\r\n                        chipLight = false,\r\n                        chipTemp = !chipTemp\">\r\n        Temperatures</v-chip>\r\n\r\n        <v-chip @click=\"chipTemp = false,\r\n                        chipMois = false,\r\n                        chipLight = false,\r\n                        chipHum = !chipHum\">\r\n        Humidity</v-chip>\r\n\r\n        <v-chip @click=\"chipTemp = false,\r\n                        chipHum = false,\r\n                        chipLight,\r\n                        chipMois = !chipMois\">\r\n        Moisture</v-chip>\r\n\r\n        <v-chip @click=\"chipTemp = false,\r\n                        chipHum = false,\r\n                        chipMois = false,\r\n                        chipLight = !chipLight\">\r\n        Light</v-chip> -->\r\n        <v-chip @click=\"chipTemp = !chipTemp\">Temperatures</v-chip>\r\n   